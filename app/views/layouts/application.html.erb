<!DOCTYPE html>
<html>
  <head>
    <title>TurbolinksPageInvalidScrollTest</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>

    <% if @turbolinks_visit_control %>
      <meta name="turbolinks-visit-control" content="<%= @turbolinks_visit_control %>">
    <% end %>
  </head>

  <body>
    <%= yield %>
  </body>
</html>
